{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/dialogflow_rules",
                "version": "8.0.2",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/wouters-frederik/dialogflow_rules.git",
                    "type": "git",
                    "reference": "8.x-1.x"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/captcha": "^1.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.6.2",
        "drupal/creative_responsive_theme": "^1.2",
        "drupal/dialogflow_rules": "^8.0",
        "drupal/entity_print": "^2.1",
        "drupal/entityreference": "1.x-dev",
        "drupal/chatbot_api": "1.x-dev",
        "drupal/api_ai_webhook": "1.x-dev",
        "drupal/flysystem_s3": "^1.0@beta",
        "drupal/geysir": "^1.1",
        "drupal/icemagic": "^1.0",
        "drupal/mbase": "^1.1",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/restui": "^1.15",
        "drupal/rules": "^3.0@alpha",
        "drupal/video": "^1.2",
        "drupal/webform": "^5.0@RC",
        "drush/drush": "^9.0.0",
        "google/cloud-dialogflow": "^0.3.2",
        "iboldurev/dialogflow": "^0.3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"

    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.6.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/chatbot_api": {
                "use Dialogflow action in stead of intent name": "https://www.drupal.org/files/issues/2018-10-30/use-action-in-stead-of-intent-name-3010190-12836053.patch"
            }
        }
    }
}
